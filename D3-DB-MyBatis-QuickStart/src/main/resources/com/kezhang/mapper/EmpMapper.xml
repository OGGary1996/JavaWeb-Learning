<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kezhang.mapper.EmpMapper">
    <!--
      namespace: 命名空间，必须对应 Mapper 接口的完整类名
      id: SQL 的唯一标识，对应接口中方法名
      resultType: 查询结果映射到的 Java 类型，指定Model的全限定类名
    -->
    <select id="getAllEmps" resultType="com.kezhang.model.Emp">
        SELECT *
        FROM employees;
    </select>

</mapper>